<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/materialize/css/materialize.min.css" rel="stylesheet" />
</head>
<body>
    <nav>
        <div class="nav-wrapper # 4db6ac teal lighten-2">
            
            <ul id="nav-mobile" class=" hide-on-med-and-down # 4db6ac teal lighten-2">
                <li><a href="#">Logo</a></li>
                <li><a href="~/Estudantes">Estudante</a></li>
                <li><a href="~/Cursos">Cursos</a></li>
                <li><a href="~/Inscricoes">Inscricoes</a></li>
            </ul>
        </div>
    </nav>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    <script src="~/Scripts/jquery-2.1.1.min.js"></script>
    <script src="~/Scripts/materialize/materialize.min.js"></script>
</body>
</html>